<include>
  <gateway name="dial-in">
    <param name="proxy" value="{{ bbb_dialin_provider }}"/>
    <param name="username" value="{{ bbb_dialin_provider_username }}"/>
    <param name="password" value="{{ bbb_dialin_provider_password }}"/>
    <param name="extension" value="{{ bbb_dialin_provider_extension }}"/>
    <param name="retry-seconds" value="10"/>
    <param name="ping" value="25"/>
    <param name="register" value="true"/>
    <param name="context" value="public"/>
  </gateway>
</include>
